    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
